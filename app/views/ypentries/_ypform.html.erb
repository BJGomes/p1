<table class="table table-striped">
	<thead>
	  	<tr>
	  		<th>Name</th>
	  		<th>Phone</th>
	  		<th>Adress</th>
	  		<th>Zip</th>
	  		<th>Parish</th>
	  		<th>Delete</th>
	  	</tr>
	</thead>
	<tbody>
	  	<% @ypentries.each do |ypentry| %>
			<tr>
				<td><%= best_in_place ypentry, :name %></td>
				<td><%= best_in_place ypentry, :phone %></td>
				<td><%= best_in_place ypentry, :address %></td>
				<td><%= best_in_place ypentry, :zip %></td>
				<td><%= best_in_place ypentry, :parish %></td>
				<td><%= link_to '<span class="glyphicon glyphicon-trash"></span>'.html_safe, ypentry, data: { confirm: 'Delete entry ' + ypentry.name + '?' }, :method => :delete %></td>
			</tr>	
		<% end %>
  	</tbody>
</table>
<%== pagy_nav_bootstrap(@pagy) %>
	