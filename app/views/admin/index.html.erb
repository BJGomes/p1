<fieldset>
    <legend> Insert new company </legend>
    <%= form_tag("/search", method: "get") do %>
        <%= label_tag(:name, "Name: ") %>
        <%= text_field_tag(:name) %>
        <%= label_tag(:tel, "Phone: ") %>
        <%= text_field_tag(:phone) %>
        </br>
        <%= label_tag(:address, "Address: ") %>
        <%= text_field_tag(:address) %>
        </br>
        <%= label_tag(:code, "Zip Code: ") %>
        <%= text_field_tag(:code) %>
        <%= label_tag(:parish, "Parish: ") %>
        <%= text_field_tag(:parish) %>
        </br>
        <%= submit_tag("Create") %>
        <%= submit_tag("Reset") %>
    <% end %>
</fieldset>
